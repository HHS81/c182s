<?xml version="1.0"?>

<PropertyList>

    <path>egt.ac</path>
    
<animation>
  <type>scale</type>
  <x-offset>0.59</x-offset>
  <y-offset>0.59</y-offset>
  <z-offset>0.59</z-offset>
 </animation>
    
    <animation>
        <type>material</type>
        <object-name>Face</object-name>
        <object-name>EGTNeedle</object-name>
        <object-name>CHTNeedle</object-name>
        <object-name>Bug</object-name>
  <emission>
   <red>1.0</red>
   <green>0.8</green>
   <blue>0.4</blue>
   <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
  </emission>
    </animation>
    
    

    <animation>
  <type>rotate</type>
  <object-name>EGTNeedle</object-name>
  <property>engines/engine/egt-degf</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>0 </dep></entry>     
   <entry><ind>1250</ind><dep> -11.67 </dep></entry>     
   <entry><ind> 1300</ind><dep> -22.40 </dep></entry>     
   <entry><ind> 1400</ind><dep>  -46.0</dep></entry>     
   <entry><ind> 1500</ind><dep>  -69.50 </dep></entry>     
   <entry><ind> 1600</ind><dep> -93.29 </dep></entry> 
   <entry><ind> 1675</ind><dep> -110.0 </dep></entry>       
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
    <center>
      <x-m> 0.001 </x-m>
      <y-m> -0.035 </y-m>
      <z-m> -0 </z-m>
    </center>
 </animation>
    
   <animation>
        <type>rotate</type>
        <object-name>CHTNeedle</object-name>
  <property>engines/engine/cht-degf</property>
 <interpolation>
      <entry><ind>0</ind><dep> 0</dep></entry>
      <entry><ind>200</ind><dep>5.51</dep></entry>
      <entry><ind>250</ind><dep>22.57 </dep></entry>
      <entry><ind>300</ind><dep>40.17</dep></entry>
      <entry><ind>350</ind><dep>57.64 </dep></entry>
      <entry><ind>400</ind><dep>74.77</dep></entry>
      <entry><ind>450</ind><dep>91.56</dep></entry>
      <entry><ind>500</ind><dep>108.5</dep></entry>
      <entry><ind>600</ind><dep>128.5</dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m> 0.001 </x-m>
      <y-m> 0.036 </y-m>
      <z-m> 0 </z-m>
    </center>
    </animation>
    
<animation>
    <type>rotate</type>
    <object-name>Bug</object-name>
  <property>engines/engine[0]/egt-bug-norm</property>
    <factor>-110.0</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
    <center>
      <x-m> 0.001 </x-m>
      <y-m> -0.035 </y-m>
      <z-m> -0 </z-m>
    </center>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>Knob.Pick.L</object-name>
    <object-name>Knob.Pick.R</object-name>

    <action>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
  <property>engines/engine[0]/egt-bug-norm</property>
        <factor>0.01</factor>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>0</wrap>
      </binding>
    </action>

    <axis>
        <x>1</x>
        <y>0</y>
        <z>0</z>
    </axis>

    <hovered>
        <binding>
            <command>set-tooltip</command>
            <tooltip-id>egt-bug-norm</tooltip-id>
            <label>EGT Bug: %.2f</label>
  <property>engines/engine[0]/egt-bug-norm</property>
        </binding>
    </hovered>
  </animation>

   

</PropertyList>
