        <menubar include="gui/dialogs/c182s-menu.xml">
          <default n="0">
            <menu n="10">
              <item n="98">
                <label>Display PFD</label>
                <binding>
                  <command>nasal</command>
                  <script>
                    var fg1000system = fg1000.FG1000.getOrCreateInstance();
                    fg1000system.displayGUI(1, 0.66);
                  </script>
                </binding>
              </item>
              <item n="99">
                <label>Display MFD</label>
                <binding>
                  <command>nasal</command>
                  <script>
                    var fg1000system = fg1000.FG1000.getOrCreateInstance();
                    fg1000system.displayGUI(2, 0.66);
                  </script>
