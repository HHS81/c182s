<comm n="0">
    <power-btn type="bool">1</power-btn>
    <frequencies>
        <dial-khz type="int">0</dial-khz>
        <dial-mhz type="int">0</dial-mhz>
    </frequencies>
    <volume-selected type="double">0.7</volume-selected>
    <channel-mode-selector type="bool">1</channel-mode-selector>
</comm>
<comm n="1">
    <power-btn type="bool">1</power-btn>
    <frequencies>
        <dial-khz type="int">0</dial-khz>
        <dial-mhz type="int">0</dial-mhz>
    </frequencies>
    <volume-selected type="double">0.7</volume-selected>
    <channel-mode-selector type="bool">1</channel-mode-selector>
</comm>
<nav n="0">
    <volume type="double">0.7</volume>
    <frequencies>
        <dial-khz type="int">0</dial-khz>
        <dial-mhz type="int">0</dial-mhz>
    </frequencies>
    <ident-audible type="bool">false</ident-audible>
</nav>
<nav n="1">
    <volume type="double">0.7</volume>
    <frequencies>
        <dial-khz type="int">0</dial-khz>
        <dial-mhz type="int">0</dial-mhz>
    </frequencies>
    <ident-audible type="bool">false</ident-audible>
</nav>

<nav-source> <!-- used by kap140 autopilot -->
    <selector type="int">0</selector>
    <in-range type="bool">false</in-range>
    <from-flag type="bool">false</from-flag>
    <to-flag type="bool">false</to-flag>
    <nav-loc type="bool">false</nav-loc>
    <has-gs type="bool">false</has-gs>
    <gs-in-range type="bool">false</gs-in-range>
    <gs-rate-of-climb type="double">0.0</gs-rate-of-climb>
    <gs-rate-of-climb-fpm type="double">0.0</gs-rate-of-climb-fpm>
    <heading-needle-deflection type="double">0.0</heading-needle-deflection>
    <heading-needle-deflection-norm type="double">0.0</heading-needle-deflection-norm>
    <gs-needle-deflection type="double">0.0</gs-needle-deflection>
    <selected-radial-deg type="double">0.0</selected-radial-deg>
    <target-radial-deg type="double">0.0</target-radial-deg>
    <course-error type="double">0.0</course-error>
</nav-source>

<transponder>
  <id-code type="int">0</id-code>
  <goodcode type="bool">0</goodcode>
  <ready type="double">0</ready>
  <replying type="bool">0</replying>

</transponder>

<dme>
  <frequencies>
    <source type="string">/instrumentation/dme/frequencies/selected-mhz</source>
    <selected-mhz type="float">113.5</selected-mhz>
    <display-sel-mhz type="int">113</display-sel-mhz>
    <display-sel-khz type="int">50</display-sel-khz>
  </frequencies>
</dme>

<davtron803>
    <serviceable type="bool">1</serviceable>
    <top-mode type="string">F</top-mode>
    <bot-mode type="string">LT</bot-mode>
    <flight-time type="string">00:00</flight-time>
    <elapsed-time type="string">00:00</elapsed-time>
</davtron803>

<!-- KAP140 internal pressure sensor -->
<altimeter-kap140-internal>
    <serviceable type="bool">true</serviceable>
</altimeter-kap140-internal>

</PropertyList>
