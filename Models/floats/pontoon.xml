    <animation>
        <type>material</type>
        <object-name>RudderCatch</object-name>
        <object-name>RudderHandle</object-name>
        <object-name>RudderScrews</object-name>
        <object-name>RudderTube</object-name>
        <object-name>RudderCable</object-name>
        <condition>
            <or>
				<not>
					<property>/sim/rendering/shaders/skydome</property>
				</not>
				<equals>
					<property>/sim/rendering/shaders/model</property>
					<value>0</value>
				</equals>
			</or>
        </condition>
        <emission>
            <red-prop>/sim/model/c182s/lighting/rgb-ra-r-factor</red-prop>
            <green-prop>/sim/model/c182s/lighting/rgb-ra-g-factor</green-prop>
            <blue-prop>/sim/model/c182s/lighting/rgb-ra-b-factor</blue-prop>
        </emission>
    </animation>

