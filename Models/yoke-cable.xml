<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>yoke-cable.ac</path>
	
	<animation>
		<type>scale</type>
		<object-name>YokeEnd</object-name>
		<object-name>YokePart</object-name>
		<object-name>MiddlePart</object-name>
		<object-name>PanelPart</object-name>
		<object-name>PanelEnd</object-name>
		<property>controls/flight/elevator</property>
		<x-factor>-0.66</x-factor>
		<x-offset>1</x-offset>
		<x-min>0</x-min>
		<x-max>2</x-max>
		<y-min>1</y-min>
		<y-max>1</y-max>
		<z-min>1</z-min>
		<z-max>1</z-max>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PanelPart</object-name>
		<object-name>MiddlePart</object-name>
		<object-name>YokePart</object-name>
		<object-name>YokeEnd</object-name>
		<axis>
			<z>1</z>
		</axis>
		<center>
			<x-m>0.01</x-m>
		</center>
		<property>controls/flight/aileron</property>
		<factor>-10</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PanelPart</object-name>
		<object-name>MiddlePart</object-name>
		<object-name>YokePart</object-name>
		<object-name>YokeEnd</object-name>
		<axis>
			<y>1</y>
		</axis>
		<center>
			<x-m>0.01</x-m>
		</center>
		<expression>
			<prod>
				<abs>
					<property>controls/flight/aileron</property>
				</abs>
				<value>-8</value>
			</prod>
		</expression>
	</animation>
</PropertyList>
