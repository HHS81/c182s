  <animation>
    <type>pick</type>
    <object-name>ap-btn</object-name>
    <action>
      <name>AP Button</name>
      <button>0</button>
      <binding>
        <command>nasal</command>
        <script>
      kap140.apButton();
     </script>
      </binding>
