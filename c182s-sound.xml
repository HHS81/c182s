            </pitch>
            <reference-dist>1.0</reference-dist>
            <max-dist>100.0</max-dist>
        </wind-right>
 
        <stall>
            <name>stall</name>
            <mode>looped</mode>
            <path>Aircraft/c182s/Sounds/stall2.wav</path>
            <condition>
                    <greater-than>
                        <property>velocities/airspeed-kt</property>
                        <value>10</value>
                    </greater-than>
                    <not>
                        <property>gear/gear[0]/wow</property> <!--stall sound audible as long nose wheel don't touch the ground-->
                    </not>
            </condition>
            <position>
                <x>-0.0360</x>
                <y>0.5174</y>
                <z>0.4831</z>
            </position>
            <volume>
                <property>/fdm/jsbsim/sounds/stall-horn-volume</property>
            </volume>
            <pitch>
                <property>/fdm/jsbsim/sounds/stall-horn-pitch</property>
                <offset>0.0</offset>
            </pitch>
            <reference-dist>0.5</reference-dist>
            <max-dist>50.0</max-dist>
        </stall>

 
